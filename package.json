{
  "name": "openusercss.org",
  "version": "0.9.6",
  "release": "alpha",
  "description": "OpenUserCSS",
  "main": "src/app.js",
  "scripts": {
    "sbs": "stmux -M -a a",
    "concurrent": "concurrently --raw --kill-others -s first",
    "contrib:add": "all-contributors add",
    "contrib:generate": "all-contributors generate",
    "clean": "rimraf app/static build && mkdir -p app/static build",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "preclient:start:prod": "yarn client:build:prod",
    "client:start:prod": "env-cmd ./.prod.env nuxt start",
    "client:start:dev": "yarn concurrent 'env-cmd ./.dev.env gulp client:watch' 'env-cmd ./.dev.env nuxt'",
    "client:build:prod": "env-cmd ./.prod.env nuxt build",
    "client:build:dev": "env-cmd ./.dev.env nuxt build",
    "api:start:dev": "yarn env-cmd ./.dev.env gulp api:watch && env-cmd ./.dev.env backpack",
    "api:start:prod": "env-cmd ./.prod.env node build/api",
    "api:build:dev": "yarn env-cmd ./.dev.env gulp api:watch && env-cmd ./.dev.env backpack build",
    "api:build:prod": "yarn env-cmd ./.dev.env gulp api:watch && env-cmd ./.prod.env backpack build",
    "lint:dry": "eslint --format=pretty --cache --ext .js,.vue --ignore-path .gitignore src tasks test app",
    "lint:fix": "eslint --format=pretty --cache --ext .js,.vue --fix --ignore-path .gitignore src tasks test app",
    "pretest:e2e": "yarn concurrent 'rimraf screenshots && mkdir -p screenshots/test/end-to-end' 'yarn build:fast'",
    "test:e2e": "yarn concurrent 'yarn start' 'wait-port -o silent 5010 && ava test/end-to-end'",
    "test:unit": "nyc ava test/unit",
    "pretest:api": "yarn api:build:dev",
    "test:api": "yarn concurrent 'yarn api:start:dev' 'wait-port -o silent 5000 && nyc ava --serial test/api'",
    "pretest:ci": "yarn api:build:dev",
    "test:ci": "yarn concurrent 'yarn api:start:dev' 'wait-port -o silent 5000 && nyc ava --serial test/api test/unit'",
    "predocker": "yarn build:prod",
    "docker": "docker build . -t decentm/openusercss",
    "postdocker": "docker push decentm/openusercss",
    "prerelease": "standard-version",
    "release": "yarn docker",
    "postrelease": "git push --follow-tags origin master",
    "prewatch": "yarn clean",
    "watch:v": "yarn sbs [ 'yarn api:start:dev' .. 'yarn client:start:dev' ]",
    "watch:h": "yarn sbs [ -r 'yarn api:start:dev' : 'yarn client:start:dev' ]",
    "commit": "git-cz",
    "postmerge": "yarn --silent --frozen-lockfile --non-interactive --production=false",
    "postcommit": "yarn --silent --frozen-lockfile --non-interactive --production=false",
    "c": "yarn commit"
  },
  "author": "DecentM",
  "license": "GPL-3.0",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
